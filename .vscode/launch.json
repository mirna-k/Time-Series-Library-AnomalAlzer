    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train TimesNet-Anomaly Detection",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}/run.py",
            "console": "integratedTerminal",
            "args": [
                "--task_name", "anomaly_detection",
                "--is_training", "1",
                "--model_id", "TimesNet_ESA-AD_y2000",
                "--model", "TimesNet",
                "--data", "ESA-AD",
                "--root_path", "./data/",
