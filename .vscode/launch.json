                "--seq_len", "1000",
                "--pred_len", "0",
                "--d_model", "64",
                "--d_ff", "8",
                "--e_layers", "2",
                "--enc_in", "1",
                "--c_out", "1",
                "--top_k", "3",
                "--batch_size", "128",
                "--train_epochs", "10",
                "--anomaly_ratio", "0.01",
                "--use_gpu", "False",
                "--gpu", "0",
            ]
        }
    ]
